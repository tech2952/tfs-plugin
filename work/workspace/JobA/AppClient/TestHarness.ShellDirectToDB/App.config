<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="Infragistics.Win.UltraWinDock.UltraDockManager.ShellForm.ultraDockManager1" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
  </configSections>

  <appSettings>
    <!--These connection strings are used by the ITaxAppInfo and ITaxAppData interfaces -->
    <add key="Main.ConnectionString" value="data source=cr-dtdv-a01;initial catalog=SampleAppTaxData;integrated security=SSPI;persist security info=False;packet size=4096"/>
    <add key="Main.ConnectionStringForDesignDatabase" value="data source=cr-dtdv-a01;initial catalog=SampleAppDesign;integrated security=SSPI;persist security info=False;packet size=4096"/>
    <add key="AppType" value="TestHarness"/>

    <add key="ITaxAppInfo.AssemblyFileName" value="TestHarness.BusinessObjects.dll" />
    <add key="ITaxAppClientData.AssemblyFileName" value="TestHarness.BusinessObjects.dll" />
    <add key="ITaxAppData.AssemblyFileName" value="TestHarness.BusinessObjects.dll" />
  </appSettings>

  <log4net>
    <appender name="Console" type="log4net.Appender.ConsoleAppender">
      <layout type="log4net.Layout.PatternLayout">
        <!-- Pattern to output the caller's file name and line number -->
        <!--<conversionPattern value="%5level [%thread] (%file:%line) - %message%newline" />-->
        <!--<conversionPattern value="%5level [%thread] %logger - %message%newline" /> -->
        <conversionPattern value="[%5level] [%thread] [%logger:%line] %message%newline" />
      </layout>
    </appender>

    <appender name="RollingFile" type="log4net.Appender.RollingFileAppender">
      <file value="TestHarnessLog.txt" />
      <appendToFile value="true" />
      <maximumFileSize value="100KB" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="2" />

      <layout type="log4net.Layout.PatternLayout">
        <header value="[LOG BEGIN %date]%newline" type="log4net.Util.PatternString" />
        <conversionPattern value="%5level [%thread] %logger:(%file:%line) - %message%newline" />
        <footer value="[LOG END %date]%newline" type="log4net.Util.PatternString" />
      </layout>
    </appender>

    <root>
      <level value="DEBUG" />
      <appender-ref ref="Console" />
      <appender-ref ref="RollingFile" />
    </root>
  </log4net>



  <userSettings>
    <Infragistics.Win.UltraWinDock.UltraDockManager.ShellForm.ultraDockManager1>
      <setting name="BinaryData" serializeAs="Xml">
        <value>
          <base64Binary></base64Binary>
        </value>
      </setting>
    </Infragistics.Win.UltraWinDock.UltraDockManager.ShellForm.ultraDockManager1>
  </userSettings>
</configuration>